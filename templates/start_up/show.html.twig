{% extends 'base.html.twig' %}

{% block title %}{{ start_up.name }}{% endblock %}

{% block titre %}{{ start_up.name }}{% endblock %}

{% block body %}

<div class="container">
    <div class="row mt-5 mb-5">
        <a class="btn btn-danger ml-5" href="{{ path('start_up_index') }}"><i class="fas fa-list"></i> Retour Ã  la liste</a>
        <a class="btn btn-danger ml-5" href="{{ path('start_up_edit', {'id': start_up.id}) }}"><i class="fas fa-pencil-alt"></i> Modifier</a>
        {{ include('start_up/_delete_form.html.twig') }}
    </div>

    <div class="row">
        <div class="table-responsive">
            <table class="table table-hover">
                <tbody>
                    <tr>
                        <th>Id</th>
                        <td>{{ start_up.id }}</td>
                    </tr>
                    <tr>
                        <th>Name</th>
                        <td>{{ start_up.name }}</td>
                    </tr>
                    <tr>
                        <th>Address</th>
                        <td>{{ start_up.address }}</td>
                    </tr>
                    <tr>
                        <th>Siret</th>
                        <td>{{ start_up.siret }}</td>
                    </tr>
                    <tr>
                        <th>Tel</th>
                        <td>{{ start_up.tel }}</td>
                    </tr>
                    <tr>
                        <th>Email</th>
                        <td>{{ start_up.email }}</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>

{% endblock %}
